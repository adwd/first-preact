{
  "name": "first-preact",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "dev": "run-p webpack-dev-server prettier-watch",
    "webpack-dev-server": "webpack-dev-server",
    "build": "mkdirp build && webpack -p",
    "prettier": "prettier {src/**/*.js,src/**/*.scss,prettier.config.js,webpack.config.js} --write",
    "prettier-watch": "onchange '{src/**/*.js,src/**/*.scss,prettier.config.js,webpack.config.js}' -- prettier --write {{changed}}"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-preact": "^1.1.0",
    "css-loader": "^0.28.8",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.0.4",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "onchange": "^3.3.0",
    "preact-render-spy": "^1.2.1",
    "prettier": "^1.9.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "~2.9.7"
  },
  "dependencies": {
    "preact": "^8.2.7",
    "preact-compat": "^3.17.0"
  },
  "jest": {
    "snapshotSerializers": [
      "preact-render-spy/snapshot"
    ],
    "moduleNameMapper": {
      "^react$": "preact-compat",
      "^react-dom$": "preact-compat",
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  }
}
